<div class="container">
	<div class="row">

		<body>
			<h1>会員一覧画面</h1>
			<table>
				<thead>
					<tr>
						<th>会員ID</th>
						<th>氏名</th>
						<th>メールアドレス</th>
						<th>ステータス</th>
					</tr>
				</thead>
				<tbody>
					<% @end_users.each do |end_user| %>
					<tr>
						<td><%= end_user.id %></td>
						<td><%= link_to end_user.full_name ,admins_end_user_path(end_user.id)  %></td>
						<td><%= end_user.email  %></td>
						<!-- if文を使ってdeleted_atがあった場合の表示したい内容　無い時の表示したい内容を書く -->
						<td>
							<% if deleted_at.present? then
								true = "退会済"
							else
								false = "有効"
							end %>
						</td>
						</tr>
						<% end %>
					</tbody>
				</table>
			</body>
		</div>
	</div>
